{
    "version":2,
    "builds":[
        {
            "src":"public/index.js",
            "use":"@vercel/node",
            "config":{
                "includeFiles":[
                    "public/schemas/**/*.graphql"
                ]
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"public/index.js"
        }
    ],
    "env":{
        "MONGO_URI":"@mongo_uri",
        "DB_NAME":"@db_name"
    }
}